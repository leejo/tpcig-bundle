{
  "name": "tpcig-bundle",
  "version": "0.0.0",
  "description": "Dynamic broadcast graphics rendered in a browser",
  "homepage": "http://nodecg.com/",
  "author": {
    "name": "Thomas Bloor",
    "email": "tbsliver@shadow.cat",
    "url": "https://tbsliver.me"
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "nodecg-bundle"
  ],
  "nodecg": {
    "compatibleRange": "~0.9.0",
    "dashboardPanels": [
      {
        "name": "sponsors",
        "title": "Sponsors",
        "width": 2,
        "file": "sponsors.html",
        "headerColor": "#9f9bbd"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720
      },
      {
        "file": "test-main.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "tpcig-sponsors",
        "title": "TPCiG Sponsors",
        "allowedTypes": [
          "png",
          "jpeg",
          "jpg",
          "gif"
        ]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git"
}
